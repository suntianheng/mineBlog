{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mineblog/src",
  "generateOptions": {
    "spec": false
  },
  "monorepo": true,
  "root": "apps/mineblog",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/mineblog/tsconfig.app.json"
  },
  "projects": {
    "mineblog": {
      "type": "application",
      "root": "apps/mineblog",
      "entryFile": "main",
      "sourceRoot": "apps/mineblog/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mineblog/tsconfig.app.json"
      }
    },
    "my-library": {
      "type": "library",
      "root": "libs/my-library",
      "entryFile": "index",
      "sourceRoot": "libs/my-library/src",
      "compilerOptions": {
        "tsConfigPath": "libs/my-library/tsconfig.lib.json"
      }
    }
  }
}